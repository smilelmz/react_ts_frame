    "fork-ts-checker-webpack-plugin": "^6.0.4",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "lint-staged": "^10.5.2",
    "mini-css-extract-plugin": "^1.3.1",
